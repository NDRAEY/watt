// imports
import 'std.io'

// a tractor ðŸšœ
type Tractor(storage) {
    // amount of ðŸŒ¾
    value := 0
    // fill ðŸŒ¿
    fun fill(value) {
        if self.value + value > storage {
            self.value = storage
            return null
        }
        self.value += value
    }
    // print ðŸ“œ
    fun print() {
        io.print('tractor value: ')
        io.println(self.value)
    }
}

tractor := new Tractor(100)
tractor.fill(50)
tractor.print()
tractor.fill(70)
tractor.print()